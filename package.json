{
  "scripts": {
    "dev": "parcel src/index.html --open",
    "build": "parcel build src/index.html --no-source-maps --no-cache",
    "clean": "npm run clean-windows || npm run clean-linux",
    "clean-linux": "rm -rf ./.parcel-cache && rm -rf ./dist",
    "clean-windows": "rmdir /Q/S ./.parcel-cache & rmdir /Q/S ./dist"
  },
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "parcel": "^2.7.0",
    "process": "^0.11.10"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
